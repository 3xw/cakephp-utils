{
  "name": "3xw/cakephp-utils",
  "description": "CakePHP utils plugins",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=8.2",
    "cakephp/cakephp": "^5.0",
    "firebase/php-jwt": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Utils\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Utils\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "minimum-stability": "dev"
}
